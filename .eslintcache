     uid:uid,\n    })\n}\n\nexport async function getUserData(uid){\n    return (await Read(firebaseCollections.users , uid)).data()\n}\n\nasync function Write(collection, doc, data) {\n    return await useFirestore()\n        .collection(collection)\n        .doc(doc).set(data);\n}\n\nasync function Read(collection, doc) {\n    return await useFirestore()\n        .collection(collection)\n        .doc(doc).get();\n}",["152","153"],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/services/firebase/collections.js",[],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/services/firebase/config.js",[],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/reportWebVitals.js",[],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/services/firebase/context.js",[],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/services/api/auth.js",[],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/services/api/controllers.js",[],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/components/ResetPassword.js",[],"/Users/Hadi/Documents/GitHub/Car-Rental-Company/src/components/Home.js",["154","155","156"],"import React from 'react'\nimport Navbar from '../layout/Navbar'\nimport '../styles/main_styles.css'\nimport Footer from '../layout/Footer'\nimport {Link} from 'react-router-dom'\nimport img1 from '../images/lamb.jpg'\nimport img2 from '../images/bug.jpg'\nimport img3 from '../images/mas.jpg'\n// style={{backgroundColor:'#e9e9e9', color:'white'}}\nexport default function Home() {\n    return (\n        <>\n            <Navbar />\n            <div class=\"jumbotron text-center mt-3 bg-white\" style={{backgroundColor:'#e9e9e9'}} >\n                <h1 class=\"display-4\">Car Rental Company</h1>\n                <p class=\"lead\">A place where you can rental your dream car!</p>\n                <hr class=\"my-4\" />\n                <p>Our mission is to provide to the costumers any cars they wish with the lowest prices in the market</p>\n                <Link className=\"btn 